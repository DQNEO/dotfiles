[include]
        path = .gitconfig.local
[user]
        # name,emailは.gitconfig.localに書く
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	di = diff
	brr = branch -r
	plf = pull --ff-only
	pi = cherry-pick
	cp = cherry-pick
	l = log --format='%C(auto)%h %Cgreen%cd%Creset %s'
[push]
	default = current
[color]
	ui = auto
[core]
	pager = lv -c
